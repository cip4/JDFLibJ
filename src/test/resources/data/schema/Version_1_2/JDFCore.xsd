	<!--The JMF types-->
	<xs:complexType name="JMFRootMessage">
		<xs:annotation>
			<xs:appinfo>
				<!--Generic-->
				<Version Path="@ResponseURL" First="1.2"/>
				<Version Path="@xmlns" First="1.1"/>
				<Version Path="./*/@xsi:type" First="1.2"/>
				<!--A JMF must have one or more child message elements -->
				<Constraint Path="./*" minOccurs="1" maxOccurs="unbounded"/>
				<Constraint Path="Command" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Acknowledge" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Response" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Query" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Signal" minOccurs="0" maxOccurs="unbounded"/>
				<!--Acknowledge-->
				<Version Path="Acknowledge/@AcknowledgeType" First="1.1"/>
				<!--Acknowledge Messages may contain 1 Notification and multiple ResponseTypeObj elements-->
				<Constraint Path="Acknowledge/Notification" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Acknowledge/*" minOccurs="0" maxOccurs="unbounded"/>
				<!--Query generic subelements-->
				<Version Path="Query/Subscription/@Format" First="1.2"/>
				<Version Path="Query/Subscription/@Template" First="1.2"/>
				<Version Path="Query/Subscription/OservationTarget/@Attributes" Last="1.1"/>
				<Version Path="Query/Subscription/ObservationTarget/@ElementType" Last="1.1"/>
				<Version Path="Query/Subscription/ObservationTarget/@ElementIDs" Last="1.1"/>
				<Version Path="Query/Subscription/ObservationTarget/@ObservationPath" First="1.2"/>
				<!--Query messages may contain 1 Subscription and multiple QueryTypeObj elements -->
				<Constraint Path="Query/Subscription" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Query/*" minOccurs="0" maxOccurs="unbounded"/>
				<!--Query generic subelements-->
				<Constraint Path="Query/Subscription/ObservationTarget" minOccurs="0" maxOccurs="unbounded"/>
				<!--Command-->
				<Version Path="Command/@AcknowledgeType" First="1.1"/>
				<Version Path="Command/@AcknowledgeFormat" First="1.2"/>
				<Version Path="Command/@AcknowledgeTemplate" First="1.2"/>
				<!--Command Messages may contain multiple CommandTypeObj elements-->
				<Constraint Path="Command/*" minOccurs="0" maxOccurs="unbounded"/>
				<!--Signal Messages-->
				<Version Path="Signal/Trigger/Added" Last="1.1"/>
				<Version Path="Signal/Trigger/ChangedAttribute" Last="1.1"/>
				<Version Path="Signal/Trigger/ChangedPath" First="1.2"/>
				<Version Path="Signal/Trigger/Removed" Last="1.1"/>
				<!--Signal Messages may contain 1 Notification, 1 Trigger and multiple QueryTypeObj and or ResponseTypeObj -->
				<Constraint Path="Signal/Notification" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Signal/Trigger" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Signal/*" minOccurs="0" maxOccurs="unbounded"/>
				<!--Signal generic subelements-->
				<Constraint Path="Signal/Trigger/Added" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Signal/Trigger/Added/*" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Signal/Trigger/ChangedAttribute" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Signal/Trigger/ChangedPath" minOccurs="0" maxOccurs="unbounded"/>
				<Constraint Path="Signal/Trigger/Removed" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Signal/Trigger/Removed/*" minOccurs="0" maxOccurs="unbounded"/>
				<!--Response messages may contain 1 Notification and multiple ResponseTypeObj elements -->
				<Constraint Path="Response/Notification" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Response/*" minOccurs="0" maxOccurs="unbounded"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="jdf:BaseElement_">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:group ref="jdf:AnyMessage" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="DeviceID" type="jdftyp:string" use="optional"/>
				<xs:attribute name="ResponseURL" type="jdftyp:URL" use="optional"/>
				<xs:attribute name="SenderID" type="jdftyp:string" use="required"/>
				<xs:attribute name="TimeStamp" type="jdftyp:dateTime" use="required"/>
				<xs:attribute name="Version" type="jdftyp:JDFJMFVersion" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Message">
		<xs:annotation>
			<xs:documentation>
        Base class of JMF messages (abstract type).
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="jdf:BaseElement_">
				<xs:attribute name="ID" type="jdftyp:ID" use="required"/>
				<xs:attribute name="Time" type="jdftyp:dateTime" use="optional"/>
				<xs:attribute name="Type" type="jdftyp:NMTOKEN" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Command">
		<xs:complexContent>
			<xs:extension base="jdf:Message">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:element ref="jdf:CommandTypeObj" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="AcknowledgeFormat" type="jdftyp:string" use="optional"/>
				<xs:attribute name="AcknowledgeTemplate" type="jdftyp:string" use="optional"/>
				<xs:attribute name="AcknowledgeURL" type="jdftyp:URL" use="optional"/>
				<xs:attribute name="AcknowledgeType" type="jdftyp:eAcknowledgeTypeList_" default="Completed"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JMFAbstractCommand_">
		<xs:complexContent>
			<xs:restriction base="jdf:Command"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Acknowledge">
		<xs:complexContent>
			<xs:extension base="jdf:Message">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:element ref="jdf:ResponseTypeObj" minOccurs="0"/>
					<xs:element name="Notification" type="jdf:NotificationAudit" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="refID" type="jdftyp:NMTOKEN" use="required"/>
				<xs:attribute name="AcknowledgeType" type="jdftyp:eAcknowledgeTypeList_" default="Completed"/>
				<xs:attribute name="ReturnCode" type="jdftyp:Integer" default="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JMFAbstractAcknowledge_">
		<xs:complexContent>
			<xs:restriction base="jdf:Acknowledge"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Response">
		<xs:complexContent>
			<xs:extension base="jdf:Message">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:element ref="jdf:ResponseTypeObj" minOccurs="0"/>
					<xs:element name="Notification" type="jdf:NotificationAudit" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="Acknowledged" type="jdftyp:boolean" default="false"/>
				<xs:attribute name="refID" type="jdftyp:NMTOKEN" use="optional"/>
				<xs:attribute name="ReturnCode" type="jdftyp:Integer" default="0"/>
				<xs:attribute name="Subscribed" type="jdftyp:boolean" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JMFAbstractResponse_">
		<xs:complexContent>
			<xs:restriction base="jdf:Response"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Signal">
		<xs:complexContent>
			<xs:extension base="jdf:Message">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:element ref="jdf:QueryTypeObj" minOccurs="0"/>
					<xs:element ref="jdf:ResponseTypeObj" minOccurs="0"/>
					<xs:element name="Notification" type="jdf:NotificationAudit" minOccurs="0"/>
					<xs:element name="Trigger" type="jdf:TriggerElement_m" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="LastRepeat" type="jdftyp:boolean" default="false"/>
				<xs:attribute name="refID" type="jdftyp:NMTOKEN" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JMFAbstractSignal_">
		<xs:complexContent>
			<xs:restriction base="jdf:Signal"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Query">
		<xs:annotation>
			<xs:appinfo>
				<Constraint Path="Subscription" minOccurs="0" maxOccurs="1"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="jdf:Message">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="jdf:GenericElements" minOccurs="0"/>
					<xs:element ref="jdf:QueryTypeObj" minOccurs="0"/>
					<xs:element name="Subscription" type="jdf:SubscriptionElement_m" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JMFAbstractQuery_">
		<xs:complexContent>
			<xs:restriction base="jdf:Query"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:group name="AnyMessage">
		<xs:sequence>
			<xs:element name="Command" type="jdf:Command" minOccurs="0"/>
			<xs:element name="Acknowledge" type="jdf:Acknowledge" minOccurs="0"/>
			<xs:element name="Response" type="jdf:Response" minOccurs="0"/>
			<xs:element name="Signal" type="jdf:Signal" minOccurs="0"/>
			<xs:element name="Query" type="jdf:Query" minOccurs="0"/>
