				<Constraint Path="Removed" minOccurs="0" maxOccurs="1"/>
				<Constraint Path="Removed/RemovedElement" minOccurs="0" maxOccurs="unbounded"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
