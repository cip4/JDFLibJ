				<xs:attribute name="Name" type="jdftyp:NMTOKEN" default="Description"/>
				<xs:attribute name="Path" type="jdftyp:PDFPath" use="optional"/>
				<xs:anyAttribute namespace="##other"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
