			<xs:documentation>
        ObjectResolutionattributes
      </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Resolution" type="jdftyp:XYPair" use="required"/>
		<xs:attribute name="SourceObjects" type="jdftyp:eSourceObjects_"/>
	</xs:attributeGroup>
