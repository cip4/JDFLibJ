		<xs:annotation>
			<xs:documentation>
        GlueLine attributes
      </xs:documentation>
		</xs:annotation>
		<xs:attribute name="Border" type="jdftyp:number" use="optional"/>
		<xs:attribute name="ForceFrontSide" type="jdftyp:NumberRangeList" use="optional"/>
		<xs:attribute name="NumberUp" type="jdftyp:XYPair" use="optional"/>
		<xs:attribute name="Rotate" type="jdftyp:number" use="optional"/>
		<xs:attribute name="FinishedPage-Orientation" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="Portrait"/>
					<xs:enumeration value="Landscape"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="PresentationDirection" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="ToBottomToRight"/>
					<xs:enumeration value="ToBottomToLeft"/>
					<xs:enumeration value="ToTopToRight"/>
					<xs:enumeration value="ToTopToLeft"/>
					<xs:enumeration value="ToRightToBottom"/>
					<xs:enumeration value="ToRightToTop"/>
					<xs:enumeration value="ToLeftToBottom"/>
					<xs:enumeration value="ToLeftToTop"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Sides" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="OneSided"/>
					<xs:enumeration value="TwoSidedLongEdge"/>
					<xs:enumeration value="TwoSidedShortEdge"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="LocationAttribs">
		<xs:annotation>
			<xs:documentation>
        Location attributes
      </xs:documentation>
		</xs:annotation>
		<xs:attribute name="LocID" type="xs:string" use="optional"/>
	</xs:attributeGroup>
