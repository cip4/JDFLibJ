<xs:schema targetNamespace="http://www.CIP4.org/JDFSchema_1_1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jdf="http://www.CIP4.org/JDFSchema_1_1" xmlns:jdftyp="http://www.CIP4.org/JDFSchema_1_1_Types" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1.a.004">
	<!--Base Elements from which all other elements are derived -->
	<xs:import namespace="http://www.CIP4.org/JDFSchema_1_1_Types" schemaLocation="JDFTypes.xsd"/>
	<xs:complexType name="EmptyElement"/>
	<xs:complexType name="Comment_Type" mixed="true">
		<xs:annotation>
			<xs:appinfo>
				<Version Path="@Attribute" First="1.1"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Attribute" type="xs:NMTOKEN" use="optional"/>
				<xs:attribute name="Box" type="jdftyp:rectangle" use="optional"/>
				<xs:attribute name="Language" type="xs:language" use="optional"/>
				<xs:attribute name="Name" type="xs:NMTOKEN" default="Description"/>
				<xs:attribute name="Path" type="jdftyp:path" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BaseElement_">
		<xs:annotation>
			<xs:appinfo>
				<Version Path="@BestEffortException" First="1.1"/>
				<Version Path="@MustHonorException" First="1.1"/>
				<Version Path="@OperatorInterventionExceptions" First="1.1"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:attribute name="BestEffortExceptions" type="xs:NMTOKENS" use="optional"/>
		<xs:attribute name="CommentURL" type="jdftyp:URL" use="optional"/>
		<xs:attribute name="DescriptiveName" type="xs:string" use="optional"/>
		<xs:attribute name="MustHonorExceptions" type="xs:NMTOKENS" use="optional"/>
		<xs:attribute name="OperatorInterventionExceptions" type="xs:NMTOKEN"/>
		<xs:anyAttribute namespace="##other" processContents="strict"/>
	</xs:complexType>
	<xs:complexType name="BaseElementForcedComment">
		<xs:sequence>
			<xs:element name="Comment" type="jdf:Comment_Type"/>
		</xs:sequence>
		<xs:attribute name="CommentURL" type="jdftyp:URL" use="optional"/>
		<xs:attribute name="DescriptiveName" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="telem">
		<xs:simpleContent>
			<xs:extension base="xs:string"/>
		</xs:simpleContent>
	</xs:complexType>
	<!--Attribute Group Section for resources-->
	<xs:attributeGroup name="ResourceElementAttribs">
		<xs:attribute name="ID" type="xs:ID" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ResourceAttribs">
		<xs:attribute name="CatalogID" type="xs:string" use="optional"/>
		<xs:attribute name="CatalogDetails" type="xs:string" use="optional"/>
		<xs:attribute name="ID" type="xs:ID" use="required"/>
		<xs:attribute name="Locked" type="xs:boolean" default="false"/>
		<xs:attribute name="PipeID" type="xs:string" use="optional"/>
		<xs:attribute name="ProductID" type="xs:string" use="optional"/>
		<xs:attribute name="rRefs" type="xs:IDREFS" use="optional"/>
		<xs:attribute name="SettingsPolicy" type="jdftyp:eSettingsPolicy_" use="optional"/>
		<xs:attribute name="SpawnIDs" type="xs:NMTOKENS" use="optional"/>
		<xs:attribute name="SpawnStatus" type="jdftyp:eSpawnStatus_" default="NotSpawned"/>
		<xs:attribute name="Status" type="jdftyp:eResourceStatus_" use="required"/>
		<xs:attribute name="UpdateID" type="xs:NMTOKEN" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ResourcePartAttribs">
		<xs:attribute name="CatalogID" type="xs:string" use="optional"/>
		<xs:attribute name="CatalogDetails" type="xs:string" use="optional"/>
