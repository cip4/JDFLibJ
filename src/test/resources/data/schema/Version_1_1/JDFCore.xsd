				<xs:attribute name="Preferred" type="xs:duration" use="optional"/>
				<xs:attribute name="Range" type="jdftyp:DurationRange" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EnumerationSpan">
