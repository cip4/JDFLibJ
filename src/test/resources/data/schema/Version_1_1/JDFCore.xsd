		<xs:attribute name="Amount" type="jdftyp:number" use="optional"/>
		<xs:attribute name="Orientation" type="jdftyp:eOrientation_" use="optional"/>
		<xs:attribute name="PipePause" type="jdftyp:number" use="optional"/>
		<xs:attribute name="PipeResume" type="jdftyp:number" use="optional"/>
		<xs:attribute name="RemotePipeEndPause" type="jdftyp:number" use="optional"/>
		<xs:attribute name="RemotePipeEndResume" type="jdftyp:number" use="optional"/>
		<xs:attribute name="Transformation" type="jdftyp:matrix" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ImplementationLinkAttribs">
		<xs:attribute name="Duration" type="xs:duration" use="optional"/>
