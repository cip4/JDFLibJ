				<xs:attribute name="Preferred" type="xs:integer" use="optional"/>
				<xs:attribute name="Range" type="jdftyp:IntegerRangeList" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NameSpan">
		<xs:complexContent>
			<xs:extension base="jdf:AbstractSpan">
				<xs:attribute name="DataType" type="jdftyp:eSpanDataType_" use="required" fixed="NameSpan"/>
				<xs:attribute name="Actual" type="xs:NMTOKEN" use="optional"/>
				<xs:attribute name="Preferred" type="xs:NMTOKEN" use="optional"/>
				<xs:attribute name="Range" type="xs:NMTOKENS" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NumberSpan">
		<xs:complexContent>
			<xs:extension base="jdf:AbstractSpan">
