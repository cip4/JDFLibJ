		<xs:attribute name="PipeProtocol" type="xs:NMTOKEN" use="optional"/>
		<xs:attribute name="PipeURL" type="jdftyp:URL" use="optional"/>
		<xs:attribute name="ProcessUsage" type="xs:string" use="optional"/>
		<xs:attribute name="rRef" type="xs:IDREF" use="required"/>
		<xs:attribute name="rSubRef" type="xs:IDREF" use="optional"/>
