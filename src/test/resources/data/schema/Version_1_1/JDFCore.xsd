		<xs:attribute name="Locked" type="xs:boolean" use="optional"/>
		<xs:attribute name="PipeID" type="xs:string" use="optional"/>
		<xs:attribute name="ProductID" type="xs:string" use="optional"/>
		<xs:attribute name="rRefs" type="xs:IDREFS" use="optional"/>
		<xs:attribute name="SettingsPolicy" type="jdftyp:eSettingsPolicy_" use="optional"/>
		<xs:attribute name="SpawnIDs" type="xs:NMTOKENS" use="optional"/>
		<xs:attribute name="SpawnStatus" type="jdftyp:eSpawnStatus_" use="optional"/>
		<xs:attribute name="Status" type="jdftyp:eResourceStatus_" use="optional"/>
		<xs:attribute name="UpdateID" type="xs:NMTOKEN" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="PhysicalResourceAttribs">
		<xs:annotation>
			<xs:appinfo>
				<Deprecated>
					<!--This attribute cannot be deprecated!
It must be removed to avoid the name clash-->&lt;xs:attribute name="Weight" type="xs:double" use="optional"/&gt;</Deprecated>
			</xs:appinfo>
		</xs:annotation>
		<xs:attribute name="AlternateBrand" type="xs:string" use="optional"/>
		<xs:attribute name="Amount" type="jdftyp:number" use="optional"/>
		<xs:attribute name="AmountRequired" type="jdftyp:number" use="optional"/>
		<xs:attribute name="BatchID" type="xs:string" use="optional"/>
		<xs:attribute name="Brand" type="xs:string" use="optional"/>
		<xs:attribute name="ResourceWeight" type="xs:double" use="optional"/>
		<xs:attribute name="Unit" type="xs:NMTOKEN" use="optional"/>
	</xs:attributeGroup>
	<!--the following attribute groups should be used by the individual resource types-->
	<xs:attributeGroup name="HandlingAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="Handling"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="QuantityAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="Quantity"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ConsumableAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="Consumable"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="PlaceHolderAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="PlaceHolder"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ImplementationAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="Implementation"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="IntentAttribs">
		<xs:attribute name="Class" type="xs:NMTOKEN" use="required" fixed="Intent"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ParameterAttribs">
		<xs:attribute name="Class" type="jdftyp:eResourceClass_" use="required" fixed="Parameter"/>
		<xs:attribute name="NoOp" type="xs:boolean" default="false"/>
	</xs:attributeGroup>
	<!--Other fundamental attribute groups-->
	<xs:attributeGroup name="AuditAttribs">
		<xs:attribute name="Author" type="xs:string" use="optional"/>
		<xs:attribute name="SpawnID" type="xs:NMTOKEN" use="optional"/>
		<xs:attribute name="TimeStamp" type="xs:dateTime" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="ResourceAuditAttribs">
		<xs:attribute name="rRefSaved" type="xs:IDREF" use="optional"/>
		<xs:attribute name="ContentsModified" type="xs:boolean" use="optional"/>
