				<xs:attribute name="DataType" type="jdftyp:eSpanDataType_" use="required" fixed="OptionSpan"/>
				<xs:attribute name="Actual" type="xs:boolean" use="optional"/>
				<xs:attribute name="Preferred" type="xs:boolean" use="optional"/>
